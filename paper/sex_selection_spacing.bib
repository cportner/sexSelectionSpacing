	Journal = {Demographic Research},
	Keywords = {Sex selective abortions},
	Month = {Aug},
	Number = {13},
	Pages = {363--416},
	Title = {Birth Spacing, Human Capital, and the Motherhood Penalty at Midlife in the United States},
	Url = {https://www.demographic-research.org/volumes/vol37/13/},
	Volume = {37},
	Year = {2017},
