@techreport{Jensen2003,
	Abstract = {Large male-female disparities in human capital outcomes are found in many developing countries, and are thought to arise from differential allocation of resources to sons and daughters within households. In this paper, we explore the role of fertility behavior as an alternative mechanism generating sex inequality. In particular, if couples have a strong desire for male offspring, they will continue childbearing until they have (many) sons (son-preferring, differential stopping behavior (SP-DSB)). This behavior leads to higher fertility overall, potentially making all children worse-off because limited household resources are shared among more children. However, this behavior will also differentially affect boys and girls; in particular, on average, girls will have more siblings than boys. As a result, girls on average share resources with larger sibling cohorts, and thus will be worse-off than boys when measured at the population-level, independent of any differential treatment by parents. In the extreme case, there can arise a situation of `equal treatment, unequal outcomes'; even if parents treated all their children equally (i.e., no differential treatment within households), aggregate statistics would still show sex disparities because girls are more likely to come from larger families where all children are worse-off. We test this hypothesis using micro-level data from India. We find strong evidence of SP-DSB; across states of India, the average girl has .10-.50 more siblings than the average boy. Using fixed-effects and instrumental variables strategies, as well as a range of self-reports of preferences and behavior, we find between onetenth to one-quarter of the large male-female differences in educational attainment in the various states can be accounted for by the differences in sibling cohort size. Furthermore, there are large, clearly identifiable subgroups for which aggregate statistics show sex inequalities in educational attainment, but for whom within-household estimates reveal no differences (i.e., equal treatment, unequal outcomes).},
	Address = {Cambridge, MA},
	Author = {Robert Jensen},
	Date-Added = {2019-02-21 15:42:24 -0800},
	Date-Modified = {2019-02-21 15:42:24 -0800},
	Institution = {John F. Kennedy School of Government, Harvard University},
	Keywords = {Sex selective abortions},
	Title = {Equal Treatement, Unequal Outcomes? Generating Sex Inequality Through Fertility Behavior},
	Type = {Working Paper},
	Url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.514.6594&rep=rep1&type=pdf},
	Year = {2003},
	Bdsk-File-1 = {}}

@article{Kugler2017,
	Abstract = {Using data from nationally representative household surveys, we test whether Indian parents make trade-offs between the number of children and investments in education. To address the endogeneity due to the joint determination of quantity and quality of children, we instrument family size with the gender of the first child, which is plausibly random. Given a strong son preference in India, parents tend to have more children if the firstborn is a girl. Our instrumental variable results show that children from larger families have lower educational attainment and are less likely to be enrolled in school, with larger effects for rural, poorer, and low-caste families as well as for families with illiterate mothers.},
