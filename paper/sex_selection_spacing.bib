	Keywords = {Sex selective abortions},
	Pages = {S50 - S60},
	Title = {Existing demand for birth spacing in developing countries: perspectives from household survey data},
	Url = {http://www.sciencedirect.com/science/article/pii/S0020729204004722},
	Volume = {89, Supplement 1},
	Year = {2005},
