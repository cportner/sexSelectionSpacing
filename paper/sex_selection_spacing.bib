	Issn = {0277-9536},
	Journal = {Social Science {\&} Medicine},
	Keywords = {India, Sex selective abortions, Oxford chapter},
	Number = {12},
	Pages = {2107 - 2119},
	Title = {Birth spacing, sibling rivalry and child mortality in India},
	Url = {http://www.sciencedirect.com/science/article/pii/S0277953602000023},
