	Date-Modified = {2017-10-17 23:06:41 +0000},
	Doi = {10.1016/j.ijgo.2004.11.012},
	Journal = {International Journal of Gynecology and Obstetrics},
	Keywords = {Sex selective abortions, Oxford chapter},
	Month = {Oct},
	Number = {S1},
	Pages = {S7--S24},
	Publisher = {Elsevier},
	Title = {Effects of preceding birth intervals on neonatal, infant and under-five years mortality and nutritional status in developing countries: evidence from the demographic and health surveys},
	Volume = {89},
	Year = {2005},
